local Old;
  Old = hookmetamethod(game, "__index", function(self, key)
    if key == "WalkSpeed" and self == game.Players.LocalPlayer.Character.Humanoid then
        return 16
    end
    return Old(self, key)
end)
		
local Old;
  Old = hookmetamethod(game, "__index", function(self, key)
    if key == "JumpPower" and self == game.Players.LocalPlayer.Character.Humanoid then
        return 50
    end
    return Old(self, key)
end)
