local Name = game:GetService("Players").LocalPlayer.Name
local Player = game:GetService("Players").LocalPlayer
local TempData = {
            ["WalkSpeed"] = 100,
            ["JumpPower"] = 100        }
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/zxciaz/VenyxUI/main/Reuploaded"))() --someone reuploaded it so I put it in place of the original back up so guy can get free credit.
local venyx = library.new("Murder Mystery 2", 5013109572)

-- themes
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}
local page = venyx:addPage("Main", 5012544693)
local section1 = page:addSection("Player Options")
local section2 = page:addSection("Main")
section1:addSlider("WalkSpeed", 0, 10, 400, function(WS)
    TempData["WalkSpeed"] = WS
end)
section1:addToggle("Enable WalkSpeed", nil, function(WS2)
   getgenv().WS2 = WS2
   if getgenv().WS2 then
       someloop = game:GetService("RunService").RenderStepped:Connect(function()
					if Player.Character and Player.Character:FindFirstChild("Humanoid") then
						Player.Character.Humanoid.WalkSpeed = TempData["WalkSpeed"]
					end
				end)
			else
			    			    					    spawn( function()
					        Player.Character.Humanoid.WalkSpeed = 16
					        end)
				someloop:Disconnect()
				end
end)
section1:addSlider("JumpPower", 0, 10, 400, function(JP2)
    TempData["JumpPower"] = JP2
end)
section1:addToggle("Enable JumpPower", nil, function(JP)
   getgenv().JP = JP
   if getgenv().JP then
       someloop = game:GetService("RunService").RenderStepped:Connect(function()
					if Player.Character and Player.Character:FindFirstChild("Humanoid") then
						Player.Character.Humanoid.JumpPower = TempData["JumpPower"]
					end
				end)
			else
			    					    spawn( function()
					        Player.Character.Humanoid.JumpPower = 50
					        end)
				someloop:Disconnect()
				end
end)
section1:addButton("Invis", nil, function(inv)
    if inv then
        local Player = game.Players.LocalPlayer
local TP = Vector3.new(-92.19873046875, 138.29556274414062, -22.79336929321289)
    if Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") and Player.Character:FindFirstChild("LowerTorso") and Player.Character.LowerTorso:FindFirstChild("Root") then
	    local P1 = CFrame.new(TP)
	    local P2 = Player.Character.HumanoidRootPart.CFrame
	    Player.Character.HumanoidRootPart.CFrame = P1
	        wait(0.15)
	    Player.Character.LowerTorso.Root:Destroy()
	        task.wait(0.2)
	    Player.Character.HumanoidRootPart.CFrame = P2
    end
    else
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end
end)

section2:addToggle("Murder and Sheriff ESP", function(esp)
    if esp then
pcall( function()
        for i, v in pairs(game.Players:GetChildren()) do
			if v:IsA("Player") and v.Backpack:FindFirstChild("Knife") or v.Character:FindFirstChild("Knife") and v:FindFirstChild("BoxHandleAdornment") == nil then
				local ESP = Instance.new("BoxHandleAdornment")
				ESP.Parent = v.Character.Head
				ESP.Size = Vector3.new(1,1,1)
				ESP.Color3 = Color3.new(255, 0, 0)
				ESP.AlwaysOnTop = true
				ESP.Adornee = v.Character.Head
				ESP.Visible = true
				ESP.ZIndex = 2
			elseif v.Backpack:FindFirstChild("Gun") or v.Character:FindFirstChild("Gun") and v.Character.Head:FindFirstChild("BoxHandleAdornment") == nil then
				local ESP2 = Instance.new("BoxHandleAdornment")
				ESP2.Parent = v.Character.Head
				ESP2.Size = Vector3.new(1,1,1)
				ESP2.Color3 = Color3.new(0, 0, 255)
				ESP2.AlwaysOnTop = true
				ESP2.Adornee = v.Character.Head
				ESP2.Visible = true
				ESP2.ZIndex = 2
		end		
        end
end)
        else
for i,v in pairs(game.Players:GetChildren()) do
    if v:IsA("Player") then
        if v.Character.Head:FindFirstChild("BoxHandleAdornment") then
            v.Character.Head.BoxHandleAdornment:Destroy()
            end
    end
    end
        end
end)
section2:addToggle("Coin ESP", nil, function(coins)
    if coins then
        getgenv().coins = true
        while getgenv().coins do wait()
            pcall( function()
for i,v in pairs(game.Workspace:GetChildren()) do
if v:IsA("Model") and v:FindFirstChild("CoinContainer") then
for i,v in pairs(v.CoinContainer:GetChildren()) do
    if v.Name == "Coin_Server" and v.Coin:FindFirstChild("C") == nil then
            local a = Instance.new("BillboardGui", v.Coin)
            a.Name = "C"
            a.AlwaysOnTop = true
            a.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
            a.Adornee = v2
            a.Size = UDim2.new(2, 1, 2)
            a.MaxDistance = math.huge
            local b = Instance.new("TextLabel", a)
            b.Name = "Coin"
            b.Text = "Coin"
            b.Font = Enum.Font.SourceSans
            b.Size = UDim2.new(0, 150, 0, 20)
            b.Position = UDim2.new(0,0,0,0)
            b.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
            b.TextStrokeTransparency = 0.8
            b.TextSize = 14
            b.ZIndex = 5
            b.BackgroundTransparency = 1
            b.TextColor3 = Color3.fromRGB(255, 255, 0)
end
end
end
end
end)
end
else
    spawn( function()
    getgenv().coins = false
    end)
                            for i,v in pairs(game.Workspace:GetChildren()) do
if v:IsA("Model") and v:FindFirstChild("CoinContainer") then
    local map = v
for i,v in pairs(map.CoinContainer:GetDescendants()) do
    if v.Name == "C" then
        wait(2)
      v:Destroy()  
    end
end
end
end
end
    end)
section2:addButton("Godmode (Permanent)", function(perm)
    if perm then
                game.Players.LocalPlayer.Character.Humanoid:Remove()
	    Instance.new('Humanoid', game.Players.LocalPlayer.Character)
	    game:GetService("Workspace")[game.Players.LocalPlayer.Name]:FindFirstChildOfClass('Humanoid').HipHeight = 2
    end
    end)
