local Name = game:GetService("Players").LocalPlayer.Name
local Player = game:GetService("Players").LocalPlayer

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("a0pg is a shit game by vez#1944 - discord.gg/SzTRYkBu8u", colors)
local Tab1 = Window:NewTab("main")
local Tab2 = Window:NewTab("teleports")

local Sec1 = Tab1:NewSection("main stuff")
local Sec2 = Tab2:NewSection("teleport stuff")

Sec1:NewButton("Copy Discord", "a", function()
    setclipboard("discord.gg/SzTRYkBu8u")
end)

Sec1:NewSlider("Speed", "speedy", 1000, 16, function(t)
    Player.Character.Humanoid.WalkSpeed = t
end)

Sec1:NewSlider("Jump", "Jumpy", 500, 50, function(t)
    Player.Character.Humanoid.JumpPower = t
end)

Sec1:NewSlider("Hip Height", "hippy nig", 50, 0, function(t)
    Player.Character.Humanoid.HipHeight = t
end)

Sec2:NewButton("Wilderness", "wild", function()
    k = workspace.Map.Islands.Wilderness.Model.Spawns.SpawnLocation
    
    local TweeningSpeed = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - k.Position).Magnitude / 5000
        game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(TweeningSpeed, Enum.EasingStyle.Linear), {
        CFrame = k.CFrame + Vector3.new(0,500,0)
    }):Play()
end)

